<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.



    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light"></style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>
    </style>
   
	<style name="style_sp_base">
	    <item name="android:spinnerMode">dropdown</item>
	    <item name="android:layout_height">@dimen/dimen_720sheight</item>
	    <item name="android:layout_marginBottom">@dimen/dimen_720minspacing</item>
	    <item name="android:textColor">@color/black</item>
	</style>

	<style name="style_login_et">
		<item name="android:drawableRight">@drawable/ic_clear_search_api_holo_light</item>
		<item name="android:background">@color/white</item>
		<item name="android:paddingLeft">@dimen/dimen_720maxspacing</item>
		<item name="android:paddingRight">@dimen/dimen_720maxspacing</item>
		<item name="android:textSize">@dimen/small</item>
	</style>
</resources>